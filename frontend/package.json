{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -u \"github-actions <github-actions@github.com>\" -r https://x-access-token:${GITHUB_TOKEN}@github.com/Marcin-Kowalczykk/CV-Marcin-Kowalczyk.git"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.1",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.16.0",
    "react": "^19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.1",
    "react-spring": "^10.0.1",
    "react-toastify": "^11.0.5",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.5",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-styled-components-a11y": "^2.2.1",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "homepage": "https://marcin-kowalczykk.github.io/CV-Marcin-Kowalczyk/"
}
